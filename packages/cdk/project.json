{
  "name": "cdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/cdk/src",
  "targets": {
    "deploy": {
      "executor": "@aws-nx/aws-cdk:deploy",
      "options": {}
    },
    "destroy": {
      "executor": "@aws-nx/aws-cdk:destroy",
      "options": {}
    },
    "bootstrap": {
      "executor": "@aws-nx/aws-cdk:bootstrap",
      "options": {}
    },
    "synth": {
      "executor": "@aws-nx/aws-cdk:synth",
      "options": {}
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/cdk/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/cdk/**/*.ts"]
      }
    }
  },
  "tags": []
}
